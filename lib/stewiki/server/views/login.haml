%h1 Login

.loginform
  %form{:action => "/login", :method => "post"}
    %p
      %input{:type => "text", :name => "login", :placeholder => "Email"}
    %p
      %input{:type => "password", :name => "password", :placeholder => "Password"}
    %p
      %input{:type => "submit", :name => "Proceed", :value => "Login"}
